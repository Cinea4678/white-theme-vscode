{
    "name": "white-next",
    "displayName": "White Next",
    "description": "Minimalist monochrome theme",
    "keywords": [
        "white",
        "theme",
        "night",
        "minimal",
        "monochrome"
    ],
    "version": "1.3.7",
    "publisher": "cinea",
    "author": {
        "name": "Arthur White"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Themes"
    ],
    "galleryBanner": {
        "color": "#000",
        "theme": "dark"
    },
    "bugs": {
        "url": "https://github.com/arthurwhite/white-theme-vscode/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/arthurwhite/white-theme-vscode"
    },
    "contributes": {
        "themes": [
            {
                "label": "White Next",
                "uiTheme": "vs",
                "path": "./themes/White-color-theme.json"
            },
            {
                "label": "White Night Next",
                "uiTheme": "vs-dark",
                "path": "./themes/White-Night-color-theme.json"
            }
        ]
    },
    "scripts": {
        "build": "./build.py"
    },
    "prettier": {
        "arrowParens": "always",
        "tabWidth": 4,
        "trailingComma": "all"
    }
}
